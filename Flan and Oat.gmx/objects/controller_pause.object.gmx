        //item equips background
        draw_roundrect_colour_ext(848, 136, 848 + 312, 616, 10, 10, c_gray, c_gray, false);
        draw_set_alpha(1);
        
        //item equips slots
        draw_roundrect_colour_ext(848 + 16, 136 + 16, 848 + 16 + 64, 136 + 16 + 64, 10, 10, c_ltgray, c_ltgray, false); 
        draw_roundrect_colour_ext(848 + 16, 136 + 16 + 72, 848 + 16 + 64, 136 + 16 + 72 + 64, 10, 10, c_ltgray, c_ltgray, false);
        draw_roundrect_colour_ext(848 + 16, 136 + 16 + 72 * 2, 848 + 16 + 64, 136 + 16 + 72 * 2 + 64, 10, 10, c_ltgray, c_ltgray, false);    
        draw_roundrect_colour_ext(848 + 16, 136 + 16 + 72 * 3, 848 + 16 + 64, 136 + 16 + 72 * 3 + 64, 10, 10, c_ltgray, c_ltgray, false);    
        draw_roundrect_colour_ext(848 + 16, 136 + 16 + 72 * 4, 848 + 16 + 64, 136 + 16 + 72 * 4 + 64, 10, 10, c_ltgray, c_ltgray, false);    
        draw_roundrect_colour_ext(848 + 16, 136 + 16 + 72 * 5, 848 + 16 + 64, 136 + 16 + 72 * 5 + 64, 10, 10, c_ltgray, c_ltgray, false);    
        
        //draws the equipped items
        if ds_exists(global.equip_grid, ds_type_grid) {
            for (var i = 0; i &lt; ds_grid_width(global.equip_grid); i++) {
                var itemID;
                itemID = ds_grid_get(global.equip_grid, i, 4);
                if itemID != "class_null_item" {
