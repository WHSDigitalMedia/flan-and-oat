        global.textScale, 0, c_white, c_white, c_white, c_white, 1);
        
        //item equip types
        draw_text_transformed_colour(848 + 16 + 72, 136 + 16 + 32, "Hat", global.textScale, global.textScale, 0, c_white, c_white, 
        c_white, c_white, 1); 
