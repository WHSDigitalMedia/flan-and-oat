        //draws the character
        draw_sprite_ext(spr_flan_idle, 3, 576, 376 - 72, 4, 4, 0, image_blend, 1);
        
        //gets the variables for the currently assigned object
        var atk = string(class_player.atk);
        var def = string(class_player.def);
        var acc = string(class_player.acc);
        var spd = string(class_player.spd);
        
        //reset the draw alignment
        draw_set_halign(fa_left);
        draw_set_valign(fa_top);
        
        //headings
        draw_text_transformed_colour(336 + 10, 136 - 40, "Flan", global.textScale, global.textScale,
        0, c_white, c_white, c_white, c_white, 1);
        draw_text_transformed_colour(848 + 10, 136 - 40, "Equipped", global.textScale, global.textScale,
        0, c_white, c_white, c_white, c_white, 1);
                
        //character stats
