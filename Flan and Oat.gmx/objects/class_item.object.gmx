            <string>//check if mouse over
if mouse_check_over(x - sprite_width/2 - padding, x + sprite_width/2 + padding, y - sprite_width/2 - padding, y + sprite_width/2 + padding) {
    //change the side panel display
    if mouse_check_button_pressed(mb_left) {
        with (data_inventory) {
            outputString = "";
            lastSpace = 0;
            strPos = 1;
            strDisplay = "";
        } 
        //set the currently selected item
        for (var i = 0; i &lt; ds_grid_width(global.inventory_grid); i++) {
            if object_get_name(object_index) == ds_grid_get(global.inventory_grid, i, 5) {
                global.curInvSelect = i;
                i = ds_grid_width(global.inventory_grid) + 1;
            }
        }
        alarm_set(0, pickupDelay);    
    }
    
    //pickup item
    if mouse_check_button(mb_left) &amp;&amp; alarm[0] == -1 &amp;&amp; global.pickup == noone { 
        //get current X and Y coords to return if dropped
        if !gotXY {
            prevX = x;
            prevY = y;
            gotXY = true;
        }
        global.pickup = self;
    } 
    
    //drop item
    if mouse_check_button_released(mb_left) &amp;&amp; global.pickup == self {
        if place_empty(x, y) {
            //return to intial slot
            x = prevX;
            y = prevY;
            global.pickup = noone;
            gotXY = false;
        } else {
            inventory_switch(); 
            global.pickup = noone;
            gotXY = false;
        }
    }
}

//drag items
if global.pickup == self {
    x = mouse_x;
    y = mouse_y;
}


</string>
