  <code>//Draw settings
draw_texture_flush();
texture_set_interpolation(false);
window_set_fullscreen(false);
var a = application_get_position();
var xx = a[0];
var yy = a[1];
var ww = a[2] - a[0];
var hh = a[3] - a[1];
if window_get_fullscreen() display_set_gui_maximise(display_get_width()/room_width, display_get_height()/room_height, xx, yy);
else display_set_gui_maximise(window_get_width()/room_width, window_get_height()/room_height, xx, yy);

//Global variables
global.keyUp = ord("W");
global.keyDown = ord("S");
global.keyRight = ord("D");
global.keyLeft = ord("A");

global.textScale = .25;
global.doorList = ds_list_create();
global.openText = noone;

//Save &amp; Load data
if file_exists("saveNumber.txt") {
    file = file_text_open_read("saveNumber.txt");
    global.saveNumber = file_text_read_real(file);
    file_text_close(file);
} else {
    global.saveNumber = 1;
    file = file_text_open_write("saveNumber.txt");
    file_text_write_real(file, global.saveNumber);
    file_text_close(file);
}
if !directory_exists(working_directory + "\saveData") directory_create("saveData");
if !directory_exists(working_directory + "\saveData2") directory_create("saveData2");
if !directory_exists(working_directory + "\saveData3") directory_create("saveData3");
if global.saveNumber == 1 {
    global.game_mainDir = working_directory + "\saveData";
} else if global.saveNumber == 2 {
    global.game_mainDir = working_directory + "\saveData2";
} else if global.saveNumber == 3 {
    global.game_mainDir = working_directory + "\saveData3";
}

//set the mouse to the center of the screen
display_mouse_set(display_get_width()/2, display_get_height()/2);

//Randomize
randomize();
//Next room
room_goto_next();</code>
